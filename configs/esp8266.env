[platformio]
data_dir = src/esp8266/data/

[env]
platform = espressif8266
framework = arduino
src_filter = +<esp8266/>
build_flags = -std=c++11
upload_speed = 460800
monitor_speed = 115200

[env:esp01]
board = esp01

[env:esp01_1m]
board = esp01_1m

[env:d1_mini]
board = d1_mini

[env:d1_mini_pro]
board = d1_mini_pro
